экспорт функция список_товаров() -> Список[Словарь[Строка, Строка]]:
    пусть кофе: Словарь[Строка, Строка] = { "id": "coffee", "название": "Кофе зерновой", "цена": "240", "остаток": "18" }
    пусть чай: Словарь[Строка, Строка] = { "id": "tea", "название": "Чай улун", "цена": "150", "остаток": "40" }
    пусть какао: Словарь[Строка, Строка] = { "id": "cocoa", "название": "Какао 80%", "цена": "190", "остаток": "21" }
    пусть печенье: Словарь[Строка, Строка] = { "id": "cookies", "название": "Овсяное печенье", "цена": "110", "остаток": "60" }
    вернуть [кофе, чай, какао, печенье]

экспорт функция цена_товара(id: Строка) -> Цел:
    если id == "coffee":
        вернуть 240
    если id == "tea":
        вернуть 150
    если id == "cocoa":
        вернуть 190
    если id == "cookies":
        вернуть 110
    вернуть 0

экспорт функция остаток_товара(id: Строка) -> Цел:
    если id == "coffee":
        вернуть 18
    если id == "tea":
        вернуть 40
    если id == "cocoa":
        вернуть 21
    если id == "cookies":
        вернуть 60
    вернуть 0

экспорт функция название_товара(id: Строка) -> Строка:
    если id == "coffee":
        вернуть "Кофе зерновой"
    если id == "tea":
        вернуть "Чай улун"
    если id == "cocoa":
        вернуть "Какао 80%"
    если id == "cookies":
        вернуть "Овсяное печенье"
    вернуть "Неизвестный товар"

экспорт функция есть_товар(id: Строка) -> Лог:
    вернуть цена_товара(id) > 0
