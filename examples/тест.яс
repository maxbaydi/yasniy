# тест.яс

функция факториал(n: Цел) -> Цел:
    если n < 0:
        печать("Ошибка: n < 0")
        вернуть 0
    пусть res: Цел = 1
    пусть i: Цел = 2
    пока i <= n:
        res = res * i
        i = i + 1
    вернуть res

функция main() -> Пусто:
    для x в диапазон(0, 6):
        печать(x, "=>", факториал(x))
    вернуть пусто
