функция факториал(n: Цел) -> Цел:
    пусть acc: Цел = 1
    пусть i: Цел = 1
    пока i <= n:
        acc = acc * i
        i = i + 1
    вернуть acc

функция main() -> Пусто:
    утверждать_равно(факториал(5), 120, "факториал(5) должен быть 120")

    пусть sum: Цел = 0
    для x в диапазон(0, 5):
        sum = sum + x

    утверждать_равно(sum, 10, "сумма диапазона 0..4 должна быть 10")
    вернуть пусто
